        <source>Access granted</source>
        <target state="final">Zugriff erlaubt</target>
      </trans-unit>
      <trans-unit id="invalidPassword" approved="yes">
        <source>Invalid password</source>
        <target state="final">Ungültiges Passwort</target>
      </trans-unit>
      <trans-unit id="userPasswordMode" approved="yes">
        <source>Use user password instead</source>
        <target state="final">Stattdessen Benutzer-Passwort verwenden</target>
      </trans-unit>
      <trans-unit id="installToolPasswordMode" approved="yes">
        <source>Use Install Tool password instead</source>
        <target state="final">Stattdessen Install-Tool-Passwort verwenden</target>
      </trans-unit>
      <trans-unit id="sudoModeUserPasswordExplanation" approved="yes">
        <source>To perform this action, please enter your user password to confirm you are authorized to proceed.</source>
        <target state="final">Gib bitte dein Benutzerpasswort ein, um diese Aktion durchzuführen und zu bestätigen, dass du berechtigt bist, fortzufahren.</target>
      </trans-unit>
      <trans-unit id="sudoModeInstallToolPasswordExplanation" approved="yes">
        <source>To perform this action, please enter the Install Tool password to confirm you are authorized to proceed.</source>
        <target state="final">Gib bitte das Passwort für das Install Tool ein, um diese Aktion auszuführen und zu bestätigen, dass du berechtigt bist, fortzufahren.</target>
      </trans-unit>
      <trans-unit id="verificationFailed" approved="yes">
        <source>Verification failed</source>
        <target state="final">Überprüfung fehlgeschlagen</target>
      </trans-unit>
      <trans-unit id="verificationExpired" approved="yes">
        <source>Confirmation has expired. Please try again.</source>
        <target state="final">Die Bestätigung ist abgelaufen. Bitte versuchen Sie es erneut.</target>
      </trans-unit>
