        <source>Cancel</source>
        <target state="final">Abbrechen</target>
      </trans-unit>
      <trans-unit id="verify" approved="yes">
        <source>Verify</source>
        <target state="final">Überprüfen</target>
      </trans-unit>
      <trans-unit id="password" approved="yes">
        <source>Password</source>
        <target state="final">Passwort</target>
      </trans-unit>
      <trans-unit id="verifyWithUserPassword" approved="yes">
        <source>Verify with user password</source>
        <target state="final">Mit dem Benutzerpasswort bestätigen</target>
      </trans-unit>
      <trans-unit id="verifyWithInstallToolPassword" approved="yes">
        <source>Verify with Install Tool password</source>
        <target state="final">Mit dem Install-Tool-Passwort bestätigen</target>
      </trans-unit>
